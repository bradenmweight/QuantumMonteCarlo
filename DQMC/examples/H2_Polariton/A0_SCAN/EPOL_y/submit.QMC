#!/bin/bash
#SBATCH -p standard
#SBATCH -J QMC
#SBATCH -o output.slurm
#SBATCH -t 1-00:00:00
#SBATCH -N 1
#SBATCH --ntasks-per-node=4
#SBATCH --mem 10GB

export OMP_NUM_THREADS=4
export MKL_NUM_THREADS=4

export QMC_HOME=/scratch/bweight/software/QuantumMonteCarlo/DQMC/src

python3 ${QMC_HOME}/main.py $1 $2